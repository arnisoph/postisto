accounts:
  gmail:
    enable: true
    connection:
      server: imap.gmail.com
      port: 993
      username: user@gmail.com
      password: <password>
      imaps: true
filters:
  gmail:
    calendar-notifications:
      commands:
        move: Tash
      rules:
        - or:
          - subject: '^(Vorl√§ufig: |Tentative: )?(Accepted: |Angenommen: |Aceptado: |Akzeptiert: |Declined: )?(Updated )?Invitation:.*'
    mailing-lists:
      commands:
        move: Lists
      rules:
        - or:
          - list-id: '.*'
    shops:
      commands:
        move: Shops
      rules:
        - or:
          - subject: Your order
    schreiner:
      commands:
        move: Shops/Holz
      rules:
        - or:
          - subject: Your Holz order
          - subject: was mit holz
